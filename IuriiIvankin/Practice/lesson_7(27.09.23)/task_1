#include <iostream>

int main() {

    setlocale(LC_ALL, "ru");
    int arr[4][4];
    for (int i = 0; i < 4; ++i)
    {
        for (int j = 0; j < 4; ++j)
        {
            arr[i][j] = i+2+j;
        }
    }

    for (int i = 0; i < 4; ++i)
    {
        for (int j = 0; j < 4; ++j)
        {
            std::cout<< arr[i][j] << ' ';
        }
        std::cout << std::endl;
    }

    return 0;
}

#include <iostream>

int main() {
    setlocale(LC_ALL, "RUS");

    int number;
    std::cout << "Введите количество очков : ";
    std::cin >> number;
    if (number >= 10)
        std::cout << "Ты победил!" << std::endl;
    else
        std::cout << "Ты не победил :(" << std::endl;
    return 0;
}

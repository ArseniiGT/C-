#include <iostream>

int main() {

    setlocale(LC_ALL, "ru");

    int a, b, c;
    std::cout << "Введите 3 числа: ";
    std::cin >> a >> b >> c;
    if (a%2 == 0 || b%2 == 0 || c%2 == 0)
    {
        if (a%2 != 0 || b%2 != 0 || c%2 != 0)
            std::cout << "YES";
        else
            std::cout << "NO";
    }
    else
        std::cout << "NO";

    return 0;
}
